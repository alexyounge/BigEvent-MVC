@model IEnumerable<BigEvent123.Models.Tickets>

@{
    ViewBag.Title = "Index";
}
                      <div class="storemanagerWrapper">
                          <div class="storemanagerHeader">
                              <h2>Index</h2>
                          </div>
                          <div class="createNewEventBtn">
                              @Html.ActionLink("Create New Ticket", "Create")
                          </div>
                          <div class="storemanagerINFO">
                              <table>
                                  <tr>
                                      <th>
                                          Event
                                      </th>
                                      <th>
                                          TicketName
                                      </th>
                                      <th></th>
                                  </tr>

                                  @foreach (var item in Model)
                                  {
                                      <tr>
                                          <td>
                                              @Html.DisplayFor(modelItem => item.Event.EventName)
                                          </td>
                                          <td>
                                              @Html.DisplayFor(modelItem => item.TicketName)
                                          </td>

                                          <td>
                                              @Html.ActionLink("Edit", "Edit", new { id = item.TicketsId }) |
                                              @Html.ActionLink("Details", "Details", new { id = item.TicketsId }) |
                                              @Html.ActionLink("Delete", "Delete", new { id = item.TicketsId })
                                          </td>
                                      </tr>
                                  }

                              </table>
                          </div>
                      </div>
